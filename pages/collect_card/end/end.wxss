/* pages/collect_card/end/end.wxss */

page {
  height: 100%;
}

.container {
  height: 100%;
  width: 100%;
  margin: 0 auto;
  text-align: center;
  background-image: linear-gradient(-132deg, #FFD600 0%, #FFBF00 100%);
}
.section {
  margin: 0 auto; 
  padding: 38rpx 0;
  text-align: center;
}
.head_text {
  display: block;
  font-size: 48rpx;
  color:  #4A4A4A;
}
.head_subtext {
  display: block;
  font-size: 32rpx;
  margin-top: 20rpx;
  color:  #4A4A4A;
}
.congratulate {
  display: inline-block;
  height: 90rpx;
  line-height: 90rpx;
  width: 576rpx;
  background: #FFFFFF;
  border-radius: 100px;
  font-size: 16px;
  color: #FFC300;
  letter-spacing: 0.8px;
}
.all_img {
  height: 480rpx;
  position: relative; 
  margin: 0 auto;
  text-align: center;
}
.img {
  position: absolute;  
  height: 468rpx;
  width: 314rpx;
  display: inline-block;
  border-radius: 14rpx; 
  box-shadow: 0 0 28rpx 0 rgba(0,0,0,0.29); 
}
.img-1 {
  left: 50rpx;
  z-index: 11;
}
.img-2 {
  left: 130rpx;
  z-index: 12;
}
.img-3 {
   left: 210rpx; 
  z-index: 15;
}
.img-4 {
  right: 130rpx;
  z-index: 14;
}
.img-5 {
  right: 50rpx;
  z-index: 13;
}
.show_coupon {
  width: 280rpx;
  height: 80rpx;
  background: #FF8D00;
  box-shadow: 0 2px 11px 0 rgba(0,0,0,0.30);
  border-radius: 25px;
  font-size: 16px;
  color: #FFFFFF;
  letter-spacing: 0.8px;
}
.rule {
  font-size: 16px;
  color: #5CA7FF;
  letter-spacing: 0.8px;
}
.view_custom_service {
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  padding: 5rpx;
}
.custom_service {
  font-size: 20rpx;
  color: #999999;
  letter-spacing: 0.8px;
  display: inline-block;
}