<import src="/utils/templete/stationList.wxml" />
<view class="container">
  <!--wxapp-toast start-->
  <view class="wxapp-toast-content-box" wx:if="{{isShowToast}}">
    <view class="wxapp-toast-content">
      <view class="wxapp-toast-content-text"> {{toastText}} </view>
    </view>
  </view>
  <!--wxapp-toast end-->
  <view class="group-coupon image-content ">
    <view class="coupon" wx:for="{{ticketGroup}}">
      <view class="coupon-title">{{item.merchandise_name}}</view>
      <view class="coupon-exp-date">有效期：{{item.limit_start_time}}至 {{item.limit_end_time}}</view>
      <view class="coupon-car-limit">{{item.use_explain}}</view>
      <view class="coupon-value">{{item.value}}</view>
    </view>

  </view>

  <navigator url="../../pages/old_active_success/old_active_success" class="button "> 继续分享 </navigator>
  <view class="login-img" style="background-color:#fff;">
    <image src="https://img.ejiayou.com/experience_app_img/experience_app_2/old_active_bg.png" mode="scaleToFill"></image>
  </view>

  <!-- 油站列表 -->
  <template is="stationListTemp" data="{{station_list: station_list}}"></template>
  
</view>